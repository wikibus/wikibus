PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX schema: <http://schema.org/>
PREFIX hydra: <http://www.w3.org/ns/hydra/core#>
PREFIX dash: <http://datashapes.org/dash#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

<>
  a sh:NodeShape ;
  rdfs:label "New vehicle"@en, "Nowy pojazd"@pl ;
  sh:property
    [
      sh:name "Brand or parent model", "Marka lub model nadrzÄ™dny"@pl ;
      sh:path skos:broader ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      dash:editor dash:AutoCompleteEditor ;
      hydra:search
      [
        hydra:template "/vehicle-concepts{?q}" ;
        hydra:mapping
          [
            hydra:variable "q" ;
            hydra:property hydra:freetextQuery ;
            sh:minLength 3 ;
          ] ;
      ] ;
      sh:order 2 ;
    ],
    [
      sh:name "Name", "Nazwa"@pl ;
      sh:path schema:name ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:minLength 1 ;
      sh:order 1 ;
    ],
    [
      sh:name "Type", "Typ"@pl ;
      sh:path rdf:type ;
      hydra:collection </product-types?super=https://schema.wikibus.org/Vehicle> ;
      sh:minCount 1 ;
      sh:maxCount 1 ;
      sh:order 3 ;
      dash:editor dash:InstancesSelectEditor ;
    ] ;
.
