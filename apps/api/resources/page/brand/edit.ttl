PREFIX schema: <http://schema.org/>
PREFIX hydra: <http://www.w3.org/ns/hydra/core#>
prefix wba: <https://wikibus.org/api#>
prefix knossos: <https://hypermedia.app/knossos#>
PREFIX hex: <https://w3id.org/hydra/extension#>

<>
  a schema:WebPage, knossos:WebPageTemplate ;
  knossos:webPageShape </api/page/Brand/Edit> ;
  knossos:pattern "/page(/brand/[^/]+)/edit" ;
  hydra:title "Edit brand" ;
  schema:hasPart </page/_element/header> ;
  schema:hasPart [ a schema:WPFooter ] ;
  schema:breadcrumb
    [
      schema:itemListElement
        [
          schema:item </> ;
          schema:name "Home" ;
          schema:nextItem
            [
              schema:item </catalog> ;
              schema:name "Catalog" ;
              schema:nextItem
                [
                  schema:item </brands> ;
                  schema:name "Brands" ;
                  schema:nextItem
                    [
                      schema:name "Edit" ;
                    ]
                ]
            ]
        ] ;
    ] ;
.
